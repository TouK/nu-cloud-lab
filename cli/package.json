{
  "name": "@nu-cloud/cli",
  "version": "0.1.0",
  "description": "CLI tool for Nu Cloud messaging - produce and consume messages",
  "type": "module",
  "bin": {
    "nu-cloud": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm --clean --shims",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nu-cloud",
    "nussknacker",
    "messaging",
    "cli",
    "producer",
    "consumer",
    "webhook"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "commander": "^11.1.0",
    "fastify": "^4.26.1",
    "axios": "^1.6.0",
    "yaml": "^2.3.4",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "inquirer": "^9.2.12"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/inquirer": "^9.0.7",
    "typescript": "^5.3.3",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
