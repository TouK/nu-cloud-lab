{
  "name": "@nussknacker/cli",
  "version": "0.0.1-beta.1",
  "description": "CLI tool for Nussknacker Cloud - produce and consume messages",
  "type": "module",
  "bin": {
    "nu-cli": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format esm --clean --shims",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "release:dry": "npx semantic-release --dry-run --no-ci",
    "check:version": "./scripts/check-version.sh"
  },
  "keywords": [
    "nu-cloud",
    "nussknacker",
    "messaging",
    "cli",
    "producer",
    "consumer",
    "webhook"
  ],
  "author": "",
  "license": "Apache-2.0",
  "dependencies": {
    "@faker-js/faker": "^10.3.0",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fastify": "^5.7.4",
    "inquirer": "^9.2.12",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.19",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "semantic-release": "^25.0.3",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
